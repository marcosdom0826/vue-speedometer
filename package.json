{
  "name": "vue-speedometer",
  "version": "1.3.1",
  "description": "Vue component for showing speedomenter like gauge with d3",
  "author": {
    "name": "palerdot",
    "email": "palerdot@gmail.com"
  },
  "main": "dist/index.js",
  "scripts": {
    "prepublish": ". ./.scripts/prepublish.sh",
    "lint": "prettier -l **/src/**/*.js",
    "lintfix": "prettier --write **/src/**/*.js",
    "test": "jest --runInBand",
    "test-watch": "npm run testonly -- --watch --watch-extensions js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o .out",
    "publish-storybook": "bash .scripts/publish_storybook.sh",
    "try-build": "NODE_ENV=production ./node_modules/.bin/babel --ignore ./src/__tests__,./src/**/*.test.js,./src/stories ./src --out-dir ./dist --no-comments",
    "make-build": "npm run try-build && cp ./src/index.d.ts ./dist/",
    "pull-submodules": "git submodule foreach git pull origin master"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/palerdot/vue-speedometer.git"
  },
  "keywords": [
    "vue",
    "speedometer",
    "gauge",
    "d3"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/palerdot/vue-speedometer/issues"
  },
  "homepage": "https://github.com/palerdot/vue-speedometer#readme",
  "dependencies": {
    "@babel/runtime": "^7.9.2",
    "@babel/runtime-corejs2": "^7.9.2",
    "d3": "^5.9.2",
    "lodash": "^4.17.14",
    "memoize-one": "^5.0.5"
  },
  "peerDependencies": {
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.0",
    "@storybook/addon-actions": "^5.3.17",
    "@storybook/addon-docs": "^5.3.17",
    "@storybook/addon-info": "^5.3.17",
    "@storybook/addon-links": "^5.3.17",
    "@storybook/addon-storysource": "^5.3.17",
    "@storybook/addons": "^5.3.17",
    "@storybook/theming": "^5.3.17",
    "@storybook/vue": "^5.3.17",
    "@vue/server-test-utils": "^1.0.0-beta.29",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-source-map-support": "2.1.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-preset-minify": "^0.5.1",
    "babel-preset-vue": "^2.0.2",
    "eslint": "^5.15.2",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "^25.1.0",
    "jsdom": "^14.0.0",
    "sprintf-js": "^1.1.2",
    "storybook-addon-vue-info": "^1.4.2",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.1",
    "vue-template-compiler": "^2.6.10"
  },
  "engines": {
    "node": ">=8.0",
    "npm": ">=3.0.0"
  }
}
